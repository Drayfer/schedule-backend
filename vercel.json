{
  "version": 2,
  "env": {
    "POSTGRES_HOST": "@POSTGRES_HOST",
    "POSTGRES_PORT": "@POSTGRES_PORT",
    "POSTGRES_USER": "@POSTGRES_USER",
    "POSTGRES_PASSWORD": "@POSTGRES_PASSWORD",
    "POSTGRES_DB": "@POSTGRES_DB",
    "TOKEN_EXP": "@TOKEN_EXP",
    "PRIVAT_KEY": "@PRIVAT_KEY",
    "PORT": "@PORT"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
    }
  ]
}
